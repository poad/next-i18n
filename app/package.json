{
  "name": "next-ts-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next dev",
    "prebuild": "pnpm install",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint -c ../eslint.config.mjs './**/*.{ts,tsx}'",
    "lint-fix": "eslint -c ../eslint.config.mjs './**/*.{ts,tsx}' --fix",
    "style-check": "prettier src",
    "format": "prettier --write src",
    "clean": "rimraf build",
    "pretest": "pnpm build",
    "all": "pnpm install && pnpm lint && pnpm test",
    "postbuild": "pnpm 404",
    "post404": "cp public/favicon.ico out",
    "prestyle-check": "pnpm install",
    "preformat": "pnpm install",
    "prelint": "pnpm style-check",
    "prelint-fix": "pnpm format",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.17",
    "i18next": "^23.11.4",
    "next": "^14.2.3",
    "next-i18next": "^15.3.0",
    "next-language-detector": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.15.17",
    "@next/bundle-analyzer": "^14.2.3",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.1",
    "autoprefixer": "^10.4.19",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "next-compose-plugins": "^2.2.1",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.12"
  }
}
