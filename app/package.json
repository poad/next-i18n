{
  "name": "next-ts-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "release": "next build && next export",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx src",
    "clean": "rimraf build",
    "pretest": "yarn build",
    "all": "yarn install && yarn lint && yarn test",
    "postbuild": "yarn 404",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/server": "*",
    "@emotion/styled": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "autoprefixer": "*",
    "next": "^12",
    "next-i18next": "*",
    "next-language-detector": "*",
    "postcss": "*",
    "react": "*",
    "react-dom": "*",
    "tailwindcss": "*",
    "web-vitals": "*"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12",
    "@swc/jest": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-config-airbnb-typescript": "*",
    "eslint-config-import": "*",
    "eslint-config-next": "^12",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "http-server": "*",
    "jest": "*",
    "next-compose-plugins": "*",
    "next-transpile-modules": "*",
    "prettier": "*",
    "rimraf": "*",
    "typescript": "*"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "optionalDependencies": {
    "@next/swc-linux-x64-gnu": "11.1.2"
  }
}
