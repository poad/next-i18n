{
  "name": "next-ts-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "test": "jest --passWithNoTests",
    "dev": "next dev",
    "prebuild": "pnpm install",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint --ext .ts .tsx src",
    "lint-fix": "eslint --ext .ts .tsx src --fix",
    "style-check": "prettier src",
    "format": "prettier --write src",
    "clean": "rimraf build",
    "pretest": "pnpm build",
    "all": "pnpm install && pnpm lint && pnpm test",
    "postbuild": "pnpm 404",
    "post404": "cp public/favicon.ico out",
    "prestyle-check": "pnpm install",
    "preformat": "pnpm install",
    "prelint": "pnpm style-check",
    "prelint-fix": "pnpm format",
    "serve": "http-server out -p 8080"
  },
  "dependencies": {
    "@emotion/cache": "*",
    "@emotion/react": "*",
    "@emotion/server": "*",
    "@emotion/styled": "*",
    "@mui/material": "*",
    "i18next": "*",
    "next": "*",
    "next-i18next": "*",
    "next-language-detector": "*",
    "react": "*",
    "react-dom": "*",
    "react-i18next": "*",
    "web-vitals": "*"
  },
  "devDependencies": {
    "@mui/icons-material": "*",
    "@next/bundle-analyzer": "*",
    "@swc/jest": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "autoprefixer": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-config-airbnb-typescript": "*",
    "eslint-config-import": "*",
    "eslint-config-next": "*",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-flowtype": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jest": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "http-server": "*",
    "jest": "*",
    "next-compose-plugins": "*",
    "postcss": "*",
    "prettier": "*",
    "rimraf": "*",
    "tailwindcss": "*",
    "typescript": "*"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.6.12"
  }
}
