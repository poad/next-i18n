{
  "name": "infra",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "lint": "eslint --ext .ts function bin lib",
    "style-check": "prettier --write function bin lib",
    "format": "prettier --write function bin lib",
    "lint-fix": "prettier --write function bin lib && eslint --fix --ext .ts function bin lib"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "aws-cdk": "^2.88.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.88.0",
    "constructs": "^10.2.69",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
