{
  "name": "infra",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "lint": "eslint -c ../eslint.config.mjs './{bin,lib,function}/**/*{.ts,.tsx}'",
    "style-check": "prettier --write function bin lib",
    "format": "prettier --write function bin lib",
    "lint-fix": "prettier --write function bin lib && eslint -c ../eslint.config.mjs --fix './{bin,lib,function}/**/*{.ts,.tsx}'"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "aws-cdk": "^2.149.0",
    "eslint": "^9.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.149.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.4"
  }
}
